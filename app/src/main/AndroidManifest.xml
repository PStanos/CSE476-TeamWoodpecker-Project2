<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.msu.cse476.teamwoodpecker_project2" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameActivity"
            android:label="@string/title_activity_game" >
        </activity>
        <activity
            android:name=".SelectionActivity"
            android:label="@string/title_activity_selection" >
        </activity>
        <activity
            android:name=".FinalScoreActivity"
            android:label="@string/title_activity_final_score" >
        </activity>
        <activity
            android:name=".AwaitActivity"
            android:label="@string/title_activity_await" >
        </activity>
        <activity
            android:name=".CreateUserActivity"
            android:label="@string/title_activity_create_user" >
        </activity>
 <!--       <activity
            android:name="edu.msu.stanospa.teamgoldfinch_project1.WaitOnUpdateActivity"
            android:label="@string/title_activity_wait_on_update" >
        </activity>  -->
        <activity
            android:name=".WaitOnUpdateActivity"
            android:label="@string/title_activity_wait_on_update" >
        </activity>
        <activity
            android:name=".WaitOnSelectActivity"
            android:label="@string/title_activity_wait_on_select" >
        </activity>
    </application>

</manifest>
